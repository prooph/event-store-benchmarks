{
  "name": "prooph/event-store-benchmarks",
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require":{
    "prooph/pdo-event-store":"^1.5.1",
    "prooph/arangodb-event-store":"dev-master",
    "prooph/arangodb-php-driver-polyfill":"dev-master",
    "psr/container":"^1.0",
    "zendframework/zend-servicemanager":"^3.3",
    "vlucas/phpdotenv": "^2.4"
  },
  "require-dev":{
    "prooph/php-cs-fixer-config": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "Prooph\\EventStoreBenchmarks\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProophTest\\EventStore\\": "vendor/prooph/event-store/tests/"
    }
  },
  "config": {
    "preferred-install": {
      "prooph/*": "source"
    }
  },
  "scripts": {
    "check": [
      "@cs",
      "@docheader"
    ],
    "docheader": "vendor/bin/docheader check examples/ src/ tests/",
    "cs": "php-cs-fixer fix -v --diff --dry-run",
    "cs-fix": "php-cs-fixer fix -v --diff"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/sandrokeil/arangodb-event-store.git"
    },
    {
      "type": "git",
      "url": "https://github.com/sandrokeil/arangodb-php-driver-polyfill"
    }
  ]
}
